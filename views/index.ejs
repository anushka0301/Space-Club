<script>
    $(function () {
    $(window).on('scroll', function () {
        if (  $(window).width() > 600 )
        {
            if ( $(window).scrollTop() > 250 ) {
                $('.container-fluid').addClass('active');
            }
            else {
                $('.container-fluid').removeClass('active');
            }
        }
        else
        {
            if ($(window).scrollTop() > 10 ) {
                $('.container-fluid').addClass('active');
            } else {
                $('.container-fluid').removeClass('active');
            }
        }
    });
});
